#nav {
  overflow: hidden;
}
#nav-container { 
  float: left;
  width: 100%;
  position: relative; 
}
#nav-container ul {
  float: left;
  list-style-type: none;
  margin: 0;
  padding: 0;
  position: relative;
  left: 50%;
}
#nav-container ul li {
  float: left;
  display: block;
  position: relative;
  right:50%;
}
#nav-container ul li a {
  text-decoration: none;
  display: block;
  padding: 10px 20px;
  color: #000;
}
#nav-container ul li a:hover {
  background-color: #a78250;
  background-image: url(nav.jfif);
  background-size: contain;
  color:#fff;
}